setwd("~/CIMAT/Tercer Semestre/Computo Estadistico/Tareas/T2")
# Crear el data frame con los datos del problema
datos_ronquidos <- data.frame(
# Puntuaciones relativas de los ronquidos como variable predictora
Ronquido_Score = c(0, 2, 4, 5),
# Número de casos CON enfermedad cardíaca (éxitos)
SI = c(24, 35, 21, 30),
# Número de casos SIN enfermedad cardíaca (fracasos)
NO = c(1355, 603, 192, 224)
)
# Verificamos la estructura de los datos
print(datos_ronquidos)
# Ajustar el modelo GLM con enlace logit
# La respuesta es una matriz de éxitos (SI) y fracasos (NO)
modelo_logit <- glm(cbind(SI, NO) ~ Ronquido_Score,
data = datos_ronquidos,
family = binomial(link = "logit"))
# Obtener un resumen detallado del modelo
summary(modelo_logit)
# Ajustar el modelo GLM con enlace probit
modelo_probit <- glm(cbind(SI, NO) ~ Ronquido_Score,
data = datos_ronquidos,
family = binomial(link = "probit"))
# Obtener un resumen detallado del modelo
summary(modelo_probit)
